void setup() {
  
 lcd.begin(16,2);
 
 lcd.clear();
 
        standingMan();
       
    lcd.setCursor(4,0);
    lcd.print("CSE 299 ");

   lcd.setCursor(4,1);
   lcd.print("project with"); 
   delay(5000);
   lcd.clear();
   standingMan();
       
    lcd.setCursor(4,0);
    lcd.print("TEAM");

   lcd.setCursor(4,1);
   lcd.print("      MICKY MOUSE"); 
   delay(5000);
        lcd.clear();
        }
        void loop() {
 
          runningManR();
         
          ManslipR();
          
          runningManL();
          
          ManslipL();
}

